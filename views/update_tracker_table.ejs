<link rel="stylesheet" type="text/css" href="/css/update_table.css"> 

<section>
    <table>
        <thead>
            <tr>
                <th>Date</th>
                <th>Day</th>
                <th>Status</th>
                <th>Streak</th>
            </tr>
            <tbody>
                <% for(tracker of all_trackers){%>
                    <tr>
                        <td><%= tracker.date%></td>
                        <td><%= tracker.day %></td>
                        <td><%= tracker.status %></td>
                        <td><%= tracker.streakCount %></td>
                    </tr>
                <%}%>
            </tbody>
        </thead>
    </table>
</section>
